.orcaDeath__graphic
	width 100%
	background rgba(255,0,0,0.1)
	/*position absolute*/
	top 0
	right 0
	transform translate3d(0, 0, 0)
	display flex
	align-items center
	
	.graphic__container
		width 100%
		max-width 60rem
		margin 0 auto
		

	.axis
		path
			stroke $off-white
		text
			fill $off-white
		line 
			stroke $off-white
			
.area
	fill $off-white
	opacity .2